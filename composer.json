{
    "name": "ahmedwaleed/laravel-soql-builder",
    "description": "An ORM solution for Salesforce Object Query Language (SOQL)",
    "keywords": [
        "AhmadWaleed",
        "laravel",
        "salesforce",
        "soql",
        "salesforce object query language",
        "orm"
    ],
    "homepage": "https://github.com/AhmadWaleed/laravel-soql-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "AhmadWaleed",
            "email": "ahmed_waleed1@hotmail.com",
            "homepage": "https://github.com/AhmadWaleed",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0",
        "omniphx/forrest": "^2.11"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "AhmadWaleed\\LaravelSOQLBuilder\\": "src",
            "AhmadWaleed\\LaravelSOQLBuilder\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmadWaleed\\SOQLBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "LaravelSOQLBuilderServiceProvider"
            ],
            "aliases": {
                "Skeleton": "SOQL\\Skeleton\\SkeletonFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
